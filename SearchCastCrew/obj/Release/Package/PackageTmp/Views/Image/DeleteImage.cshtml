@model SearchCastCrew.Models.ImageToDeleteVm
@{
    //Layout = null;
}




<div class="col-md-6 col-md-offset-3 margin-edit">
    <h3>Are you sure you want to delete this image?</h3>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        @Html.HiddenFor(model => model.ImageId)

        <div class="form-actions no-color">
            @*<input type="submit" value="DeleteImage" class="btn btn-default" /> |*@

            <input type="submit" value="Delete" class="btn btn-default" />
            <button class="btn btn-default" onclick="goBack()">Cancel</button>
        </div>
    }
</div>
<style>
    .margin-edit {
        margin-top: 100px;
    }
</style>